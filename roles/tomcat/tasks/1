---
 - name: Launch the new EC2 Instance
   local_action: ec2 
                 group={{ security_group }} 
                 instance_type={{ instance_type}} 
                 image={{ image }} 
                 wait=true 
                 region={{ region }} 
                 keypair={{ keypair }}
                 count={{count}}
                 user_data: "{{ lookup('file', 'user_data.sh') }}"
   register: ec2
